        <target>Accesso non effettuato.</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="URL">
        <source>URL</source>
        <target>URL</target>
        <note>Accessibility label</note>
      </trans-unit>
      <trans-unit id="Web content">
        <source>Web content</source>
        <target>Contenuti web</target>
        <note>Accessibility label for the main web content view</note>
      </trans-unit>
