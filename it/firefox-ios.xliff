        <target>Inizia</target>
        <note>Get started button</note>
      </trans-unit>
      <trans-unit id="OK">
        <source>OK</source>
        <target>OK</target>
        <note>OK button</note>
      </trans-unit>
      <trans-unit id="Reader">
        <source>Reader</source>
        <target>Lettore</target>
        <note>Browser function that presents simplified version of the page with bigger text.</note>
      </trans-unit>
      <trans-unit id="Search or enter address">
        <source>Search or enter address</source>
        <target>Cerca o inserisci un indirizzo</target>
        <note>The text shown in the URL bar on about:home</note>
      </trans-unit>
      <trans-unit id="Secure connection">
        <source>Secure connection</source>
        <target>Connessione sicura</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Send To Device">
        <source>Send To Device</source>
        <target>Invia a dispositivo</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Send to ">
